list_1=['physics', 'chemistry', 'math', 'biology']
list_2=list_1
print(list_1)
print(list_2)
list_1[0]='arts'
print(list_1)
print(list_2)

tuples_1=('physics', 'chemistry', 'math', 'biology')
tuples_2=tuples_1
print(tuples_1)
print(tuples_2)
tuples_1[0]='arts'
print(tuples_1)
print(tuples_2)
# this code will give an error at the last part after changing the value of the tuple, because tuples are non-mutable.
